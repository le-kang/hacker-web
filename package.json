    "expo-status-bar": "~1.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-autoheight-webview": "^1.6.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-web": "0.17.1",
    "react-native-webview": "11.15.0",
    "swr": "^1.3.0",
    "tailwind-rn": "^4.2.0",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
